alias stopohs='sh /opt/ucm/u01/app/oracle/product/TA/SCRIPTS/WCC_OPS/stopohs.sh'

#####################################################################################################################################
# Alias for Coherence
#####################################################################################################################################
alias cohloc="cd $COHERENCE_HOME/bin"
alias cohlog="cd /var/rigcache/logs"
alias opsloc="cd /opt/rigcache/application/rig-authorisation/ops"
alias runsync="cd /opt/rigcache/application/rig-authorisation/bin && yes | ./rigauth-sync-tool.sh default"
alias startcoh="cd /opt/rigcache/application/rig-authorisation/bin && yes | ./rigauth-cluster-control.sh start"
alias stopcoh="cd /opt/rigcache/application/rig-authorisation/bin && yes | ./rigauth-cluster-control.sh stop"
alias statuscoh="cd /opt/rigcache/application/rig-authorisation/ops/ && ./coherence_ops.sh HC"
alias accountsize="cd /opt/rigcache/application/rig-authorisation/bin && yes | ./rigauth-operations.sh size account"
alias assetsize="cd /opt/rigcache/application/rig-authorisation/bin && yes | ./rigauth-operations.sh size asset"
alias adddata="cd /opt/rigcache/application/rig-authorisation/ops/ && ./coherence_ops.sh add"
alias purgelogs="find /var/rigcache/logs -mtime +2 -type f -delete"
