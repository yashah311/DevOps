alias stopovd="cd /opt/ucm/ovd/oracle/product/asinst_1/bin && ./opmnctl stop"
alias mwhome="cd /opt/ucm/u01/app/oracle/product/12C/"
alias mwlogs="cd /opt/ucm/u01/app/oracle/product/LOGS"
