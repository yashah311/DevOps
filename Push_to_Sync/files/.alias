alias startEMNodeManager='cd /opt/ucm/u01/app/oracle/product/12C/wlserver/server/bin; nohup ./startNodeManager.sh & > /opt/ucm/u01/app/oracle/product/LOGS/ECM/startECMNodeManag.log 2>&1'
